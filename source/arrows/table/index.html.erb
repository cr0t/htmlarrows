<%# Meta Tags %>
<% content_for :meta do %>
  <%= partial("meta/meta_index") %>
<% end %>

<%# Colors %>
<% content_for :stylesheets do %>
	article {background-color: rgb(45, 50, 55);}
<% end %>

<%# Page Content %>
<section class="row index arrows">
	<table>
		<thead>
			<tr>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% data.arrows.entities.each do |entity| %>
			<tr class="col-xs-12 entity" vocab="http://schema.org/" typeof="Code">
				<td class="name">
					<h5 property="name"><%= entity.name %></h5>
				</td>

				<td class="symbol" title="<%= entity.name %>">
					<% link_to "#{current_page.url}#{entity.slug}/" do %>
						&<%= entity.html_code %>
					<% end %>
				</td>
				<td><code style="float:left;" title="Unicode" property="sampleType"><%= entity.unicode %></code></td>
				<td><code style="float:left;" title="Hex Code" property="sampleType">&amp;<%= entity.hex_code %></code></td>
				<td><code style="float:left;" title="HTML Code" property="sampleType">&amp;<%= entity.html_code %></code></td>
			</tr>
			<% end %><br>
		</tbody>
	</table>

</section>

<%# Page Footer %>
<% content_for :footer do %>
	<section class="row breadcrumbs">
		<div class="col-xs-24" xmlns:v="http://rdf.data-vocabulary.org/#">
			<h6>
			  <span typeof="v:Breadcrumb">
			  	<%= link_to "Home", "/", rel: "v:url", property: "v:title" %> &#8594;
			    <span rel="v:child" typeof="v:Breadcrumb">
			      <%= link_to "Arrows", "/arrows/", rel: "v:url", property: "v:title" %>
			    </span>
			  </span>
			 </h6>
		</div>
	</section>
<% end %>