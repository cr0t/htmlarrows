<%# Meta Tags %>
<% content_for :meta do %>
  <%= partial "meta/meta_index" %>
<% end %>

<%# Page Content %>
<section class="row">

	<% data.arrows.entities.each do |entity| %>
		<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 col-xl-2 entity" vocab="http://schema.org/" typeof="Code">
			<div class="name small">
				<h4 property="name"><%= entity.name %></h4>
			</div>

			<div class="symbol small">
				&<%= entity.html_code %>
			</div>

			<div class="codes small">
				<code><%= entity.unicode %></code>
				<code>&amp;<%= entity.hex_code %></code>
				<code>&amp;<%= entity.html_code %></code>
			</div>
		</div>
	<% end %>

</section>

<%# Page Footer %>
<footer class="row">
	<div class="col-xs-24">
		<h6>Home > Currency</h6>
	</div>
</footer>